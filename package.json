{
  "name": "@tabetalt/pulumix",
  "main": "dist/index.js",
  "description": "Pulumix is a set of template-driven Pulumi resources, used to configure Tabetalts intrastructure as code.",
  "license": "MIT",
  "version": "1.0.0",
  "private": false,
  "keywords": [
    "pulumi",
    "templates",
    "infrastructure",
    "infrastructure as code"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "generate-barrels": "node scripts/barrels.js; prettier --write src/",
    "generate-docs": "typedoc --mode file --readme none --plugin typedoc-plugin-markdown --hideSources --out docs src",
    "postbuild": "yarn generate-docs"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "prettier": "^2.0.5",
    "typedoc": "^0.17.3",
    "typedoc-plugin-markdown": "^2.2.16",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@pulumi/docker": "^1.5.0",
    "@pulumi/gcp": "^3.1.0",
    "@pulumi/kubernetes": "^2.0.0",
    "@pulumi/pulumi": "^2.0.0",
    "@pulumi/random": "^2.0.0",
    "@pulumi/vault": "^1.11.0"
  }
}
